
router ospf 1
   router-id 192.168.255.23
   max-lsa 12000
!
!
interface Loopback0
   ip address 192.168.255.23/32
   ip ospf area 0.0.0.0
!
interface Loopback1
   ip address 192.168.255.134/32
   ip ospf area 0.0.0.0
!
interface Port-Channel1
   switchport mode trunk
   switchport trunk group mlag-peer
!
vlan 4094
   trunk group mlag-peer
!
interface Vlan4094
   ip address 10.10.0.49/30
   no autostate
!
ip virtual-router mac-address de:ad:be:ef:fa:dd
!
!
mlag configuration
   domain-id mlag2
   heartbeat-interval disabled
   local-interface Vlan4094
   peer-address 10.10.0.50
   peer-link Port-Channel1
!
