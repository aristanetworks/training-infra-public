!
vlan 101,201
!
interface Ethernet1
   switchport access vlan 101
!
interface Ethernet2
   switchport access vlan 201
!
interface Ethernet3
   shutdown
!
interface Ethernet4
   shutdown
!
interface Vlan101
   no autostate
   ip address 192.168.101.1/24
!
interface Vlan201
   no autostate
   ip address 192.168.201.1/24
!
ip route 192.168.202.0/24 192.168.201.254
!