key = "COOKBOOK"
group = limited {
   default service = deny
}
user = fedexuser {
    # default service = permit
    login = clear fedexuser
    # member = limited
    service = shell {
      set priv-lvl = 15
      cmd = show {
        deny running-config
        permit .*
     }
   }
}
